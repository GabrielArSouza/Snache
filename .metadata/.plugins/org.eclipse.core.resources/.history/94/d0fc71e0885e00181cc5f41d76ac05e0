package domain;

public class BoardPiece
{
	private int row;
	private int column;
	private boolean empty;
	
	public BoardPiece(int row, int column)
	{
		this.row = row;
		this.column = column;
		this.empty = true;
	}
	
	public boolean isEmpty()
	{
		return empty;
	}

	public void setRow(int row)
	{
		this.row = row;
	}

	public void setColumn(int column)
	{
		this.column = column;
	}
}
