package domain;

public class BoardPiece
{
	private int row;
	private int column;
	private boolean empty;
	
	public BoardPiece(int row, int column)
	{
		this.row = row;
		this.column = column;
		this.empty = true;
	}
	
	public boolean isEmpty()
	{
		return empty;
	}
	
	public int getX()
	{
		return x;
	}
	
	public int getY()
	{
		return y;
	}
	
	public void setX(int x)
	{
		this.x = x;
	}
	
	public void setY(int y)
	{
		this.y = y;
	}
	
}
